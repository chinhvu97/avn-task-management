{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build:all",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/hq",
      "destination": "/ai-hq-task-assignment/build/index.html"
    },
    {
      "source": "/hq/:path*",
      "destination": "/ai-hq-task-assignment/build/:path*"
    },
    {
      "source": "/staff",
      "destination": "/staff-task-management/build/index.html"
    },
    {
      "source": "/staff/:path*",
      "destination": "/staff-task-management/build/:path*"
    },
    {
      "source": "/",
      "destination": "/ai-hq-task-assignment/build/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}
